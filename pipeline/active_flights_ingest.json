{
	"name": "active_flights_ingest",
	"properties": {
		"activities": [
			{
				"name": "Copy data from API to blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "eal_ac_src_active_flights",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "eal_ac_active_flights",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Data Flow Active Flights",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Copy data from API to blob",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "active_flight_dataflow",
						"type": "DataFlowReference",
						"parameters": {
							"run_id": {
								"value": "'@{pipeline().RunId}'",
								"type": "Expression"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					}
				}
			}
		],
		"parameters": {
			"run_id": {
				"type": "string",
				"defaultValue": "@pipeline().RunId"
			}
		},
		"variables": {
			"ADF_run_ID": {
				"type": "String"
			}
		},
		"annotations": []
	}
}